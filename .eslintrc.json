{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "quotes": 0,
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0
  }
}
